<%= simple_form_for @job do |f| %>
  <%= f.input :title,label:"your novel name"%>
  <%= f.input :description,placeholder:"tell more details"%>
  <%= f.input :wage_upper_bound,hint:"only integer"%>
  <%= f.input :wage_lower_bound%>
  <%= f.input :contact_email %>
  <%= f.input :is_hidden %>
  <%= f.button :submit, class:"btn btn-primary"%>
<% end %>
